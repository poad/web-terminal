{
  "name": "webshell",
  "version": "1.0.0",
  "private": true,
  "description": "shell in your browser",
  "dependencies": {
    "@types/express": "*",
    "@types/node": "*",
    "@types/pty.js": "*",
    "@types/socket.io": "*",
    "@types/ws": "*",
    "express": "*",
    "express-ws": "*",
    "lodash": "*",
    "node-pty": "*",
    "ts-node": "*",
    "utf8": "*",
    "ws": "*",
    "xterm": "*",
    "xterm-addon-attach": "*",
    "xterm-addon-fit": "*",
    "xterm-addon-search": "*",
    "xterm-addon-web-links": "*",
    "yarn": "*"
  },
  "devDependencies": {
    "ts-loader": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-cli": "*"
  },
  "main": "app.js",
  "scripts": {
    "setup": "yarn run build",
    "prepare": "yarn run setup",
    "build": "tsc --outDir . src/app.ts",
    "prepackage": "yarn run build",
    "package": "webpack --config webpack.config.js",
    "prepublishOnly": "yarn run package",
    "start": "node app.js"
  },
